32","33"],"import React, {useEffect, useState} from \"react\";\nimport Card from \"react-bootstrap/Card\";\nimport Button from \"react-bootstrap/Button\";\nimport {Link, useParams} from \"react-router-dom\";\n\nfunction chunk(arr, len) {\n  var chunks = [],\n    i = 0,\n    n = arr.length;\n\n  while (i < n) {\n    chunks.push(arr.slice(i, i += len));\n  }\n\n  return chunks;\n}\n\nconst NotesEdit = () => {\n  const [title, setTitle] = useState(null);\n  const [content, setContent] = useState(null);\n  const [createdAt, setCreatedAt] = useState(null);\n  const {id} = useParams();\n\n  const submit = () => {\n    fetch(\n      `http://192.1.9.2/notes/${id}`,\n      {\n        method: 'PUT',\n        body: JSON.stringify({\n          title: title,\n          content: content\n        }),\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }\n    )\n  }\n\n  useEffect(() => {\n    fetch(`http://192.1.9.2/notes/${id}`)\n      .then(response => response.json())\n      .then(response => {\n        setTitle(response.data.title)\n        setContent(response.data.content)\n        setCreatedAt(response.data.createdAt)\n      })\n  }, [id])\n\n  return (\n    <div>\n      <Card>\n        <Card.Body>\n          <Card.Title>\n            <input type=\"text\" onChange={(event => setTitle(event.target.value))} value={title}/>\n          </Card.Title>\n          <Card.Text>\n            Created at {createdAt}\n          </Card.Text>\n          <Card.Text>\n            <textarea name=\"\" id=\"\" cols=\"60\" rows=\"20\" onChange={(event => setContent(event.target.value))} value={content}/>\n          </Card.Text>\n\n          <Button variant=\"primary\" onClick={()=>{submit()}}>Save</Button>\n        </Card.Body>\n      </Card>\n    </div>\n  )\n}\n\nexport default NotesEdit;\n","/media/gh05t/ASD/MY_PROJECTS/STUDIA/notes/front/src/components/NotesAdd.js",["34","35","36","37"],{"ruleId":"38","severity":1,"message":"39","line":1,"column":8,"nodeType":"